{
  "name": "chin-crm",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch server/_core/index.ts",
    "build": "vite build && esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.js && esbuild server/scripts/migrate.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/migrate.js",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "pnpm check:client && pnpm check:server",
    "check:client": "tsc --noEmit -p tsconfig.json",
    "check:server": "tsc --noEmit -p tsconfig.server.json",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "cross-env NODE_ENV=test ALLOW_MOCK_DB=1 USE_MOCK_DB=true vitest run --reporter=dot",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node dist/migrate.js",
    "bootstrap:admin": "tsx server/scripts/bootstrap-admin.ts",
    "bootstrap:admin:prod": "node dist/bootstrap-admin.js",
    "set-owner": "tsx server/scripts/set-owner.ts",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "playwright install --with-deps",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "validate:prod-config": "tsx server/scripts/validate-prod-env.ts",
    "verify:tenant-indexes": "tsx server/scripts/verify-tenant-indexes.ts",
    "verify:tenant-performance": "tsx server/scripts/verify-tenant-performance.ts",
    "test:real-db": "cross-env RUN_REAL_DB_PARITY=1 vitest run tests/integration/real-db-parity.test.ts",
    "hooks:install": "git config core.hooksPath .githooks",
    "secrets:scan": "gitleaks detect --no-git --source . --redact --config .gitleaks.toml"
  },
